<div class="container">      
    <div class="row">
    <br>
    <a href="/register/new" class="btn btn-primary">Add New</a>
      <table class="table table-striped">
        <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone #</th>
            <th>Password</th>

            <th colspan="2">Actions</th>
        </tr>
        </thead>
        {{#each registers}}
          <tr>
            <td>{{this.firstName}}</td> 
            <td>{{this.lastName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phoneNumber}}</td>
            <td>{{this.password}}</td>
            <td><a href="/register/update/{{this._id}}">Update</a></td> 
            <td><a href="/register/delete/{{this._id}}">Delete</a></td> 
          </tr>          
       {{/each}}   
      </table>
    </div>
</div>
